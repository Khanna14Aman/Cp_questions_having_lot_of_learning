question==>https://codeforces.com/contest/1592/problem/A

#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n,h;
        cin>>n>>h;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        sort(a,a+n);
        int maxi = a[n-1];
        int maxi2 = a[n-2];
        int ans;
        if(h%(maxi+maxi2)==0){
            ans = 2*(h/(maxi+maxi2));
        }
        else if(h%(maxi+maxi2)<=maxi){
            ans = 2*(h/(maxi+maxi2))+1;
        }
        else{
            ans = 2*(h/(maxi+maxi2))+2;
        }
        cout<<ans<<"\n";
    }
    return 0;
}
