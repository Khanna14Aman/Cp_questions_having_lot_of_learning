read editorial

#include<bits/stdc++.h>
using namespace std;
#define int long long
signed main(){
    int t;
    cin>>t;
    while(t--){
        int n,x;
        cin>>n>>x;
        int a[n];
        int b[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
            b[i]=a[i];
        }
        sort(b,b+n);
        string ans = "YES";
        for(int j = n-x;j<x;j++){
            if(a[j]!=b[j]){
                ans = "NO";
                break;
            }
        }
        cout<<ans<<"\n";
    }
    return 0;
}
