//question==>https://www.codechef.com/COOK130C/problems/WAV2
//intution==>   (x-a1)*(x-a2)*(x-a3)*.................*(x-an)  and find what is final answer it may be 0 or +ve or -ve. 
#include <iostream>
#include<bits/stdc++.h>
#define int long long 
using namespace std;

signed main() {
    int n,q,i;
    cin>>n>>q;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    sort(a,a+n);
    while(q--)
    {
        int x;
        cin>>x;
        int ind = lower_bound(a,a+n,x)-a;
        if(ind<n && a[ind]==x)//since if xi is present in array then xi-ai will be 0 so all our final answer will be 0 
        {
            cout<<"0\n";
        }
        else if(ind<n && a[ind]!=x)// if xi is not present in array but number greater to it are preset in array so the total no of element greater to it is even so answer will be +ve else -ve.
        {
            if((n-ind)%2==0)
            {
                cout<<"POSITIVE\n";
            }
            else
            {
                cout<<"NEGATIVE\n";
            }
        }
        else if(x> a[n-1])// if xi is greater than last element this means xi is greater than every element of array so for every ai   (xi-ai) will be positive;
        {
            cout<<"POSITIVE\n";
        }
        else if(x<a[0])//if xi is less than smallest element of array than every (x-ai) will be -ve so answer will depend on no of -ve element we get after every (x-ai); if -ve is even this means final answer will be +ve because -2*-2 = 4;
        {
            if(n%2==0)
            {
                cout<<"POSITIVE\n";
            }
            else
            {
                cout<<"NEGATIVE\n";
            }
        }
    }
	// your code goes here
	return 0;
}
