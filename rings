question==>https://codeforces.com/contest/1562/problem/C
#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n,i,j;
        cin>>n;
        string s;
        cin>>s;
        int solved=0;
        for(i=0;i<n;i++){
            if(s[i]=='0'){
                solved=1;
                if(i+1>n/2){
                    cout<<"1 "<<i+1<<" 1 "<<i<<"\n";
                    break;
                }
                else{
                    cout<<i+1<<" "<<n<<" "<<i+2<<" "<<n<<"\n";
                    break;
                }
            }
        }
        if(!solved){
            cout<<"1 "<<n-1<<" "<<"2 "<<n<<endl;
        }
    }
    return 0;
}
