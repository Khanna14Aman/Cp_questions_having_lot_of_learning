question==>https://www.codechef.com/START12C/problems/MAKEDIV3

solution==>https://www.codechef.com/viewsolution/51446261

#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string s="";
        if(n%3==0){
            for(int i=1;i<=n-1;i++){
                s = s+'3';
            }
            s = s+'9';
        }
        else{
            for(int i=1;i<=n;i++){
                s=s+'3';
            }
        
        }
        cout<<s<<"\n";
    }
	// your code goes here
	return 0;
}
