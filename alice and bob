video==>https://youtu.be/5Xt-Z3c7Qbs

#include <iostream>
#include<bits/stdc++.h>
#define int long long 
using namespace std;

int fun(int n){
    if(n==0)return 0;
    if(n%2 != 0)return !fun(n-1);
    if(n%4 != 0)return !fun(n/2);
    return 1;
}

signed main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        if(fun(n))cout<<"Alice\n";
        else cout<<"Bob\n";
    }
	// your code goes here
	return 0;
}
